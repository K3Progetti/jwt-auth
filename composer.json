{
    "name": "k3progetti/jwt-auth",
    "type": "library",
    "description": "Symfony jwt auth bundle. Comes with refresh token enabled by default",
    "keywords": [
        "jwt",
        "auth",
        "symfony-auth"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Esc\\JwtAuth\\": "src/"
        }
    },
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "k3progetti/user": "^4.0",
        "gesdinet/jwt-refresh-token-bundle": "^0.9",
        "lexik/jwt-authentication-bundle": "^2.6",
        "psr/log": "^1.1",
        "symfony/http-foundation": "^5.1",
        "symfony/security-bundle": "^5.1"
    },
    "config": {
        "sort-packages": true
    },
    "version": "4.0.6"
}
